{
  "name": "fitness-tracker-app",
  "version": "1.0.0",
  "description": "A web application for tracking fitness goals and sharing progress.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "vite",
    "server": "nodemon server.js"
  },
  "nodemonConfig": {
    "watch": [
      "api/**",
      "server.js"
    ]
  },
  "type": "module",
  "dependencies": {
    "axios": "1.7.9",
    "bcrypt": "5.1.1",
    "express": "4.21.2",
    "jsonwebtoken": "9.0.2",
    "mongodb": "6.12.0"
  },
  "devDependencies": {
    "concurrently": "8.2.2",
    "eslint": "9.17.0",
    "jest": "29.7.0",
    "@testing-library/react": "16.1.0",
    "nodemon": "3.1.1",
     "prettier": "3.4.2",
    "react-router-dom": "7.1.1",
    "react": "19.0.0",
    "react-hook-form": "7.54.2",
    "react-dom": "19.0.0",
    "tailwindcss": "3.4.17",
    "vite": "6.0.6"
  }
}